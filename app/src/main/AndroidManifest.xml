<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ProjectManagerOnline">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Event Manager Online"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectManagerOnline">
        <activity
            android:name=".viewPlans"
            android:label="View Plans"
            android:parentActivityName=".viewPlans"
            android:exported="true" />
        <activity
            android:name=".viewEditDeleteTicket"
            android:exported="true"
            android:label="Ticketing"
            android:parentActivityName=".Ticketing" />
        <activity
            android:name=".ViewWork"
            android:exported="true"
            android:label="View Work"
            android:parentActivityName=".postevent" />
        <activity
            android:name=".postevent"
            android:exported="true" />
        <activity
            android:name=".Ticketing"
            android:exported="true" />
        <activity
            android:name=".marketingplan"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>