<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ProjectManagerOnline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="EventManagement"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectManagerOnline">
        <activity
            android:name=".ViewWork"
            android:exported="true"
            android:label="View Work"
            android:parentActivityName=".meal"/>
        <activity
            android:name=".RecyclerView"
            android:exported="true"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".addData"
            android:exported="true" />
        <activity
            android:name=".meal"
            android:exported="true" />
        <activity
            android:name=".body"
            android:exported="true" />
        <activity
            android:name=".habit"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>